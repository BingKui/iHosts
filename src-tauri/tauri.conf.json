{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "yarn build",
    "beforeDevCommand": "yarn dev",
    "devPath": "http://localhost:5173",
    "distDir": "../dist"
  },
  "package": {
    "productName": "iHost",
    "version": "1.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "os": {
        "all": true
      },
      "shell": {
        "all": true,
        "execute": true,
        "open": true,
        "scope": [{
          "name": "networksetup",
          "cmd": "networksetup",
          "args": true
        }],
        "sidecar": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "Copyright © 2022 康兵奎",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/icon.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "cn.uiseed.ihost",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": "康兵奎",
        "signingIdentity": "Apple Distribution: Bingkui Kang (N8CP79P49X)"
      },
      "resources": [],
      "shortDescription": "好用的中文host文件内容管理工具。",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "systemTray": {
      "iconPath": "icons/tray.png",
      "iconAsTemplate": true
    },
    "macOSPrivateApi": false,
    "windows": [{
      "fullscreen": false,
      "height": 700,
      "resizable": false,
      "transparent": true,
      "title": "iHost",
      "width": 900,
      "decorations": false,
      "visible": false
    }]
  }
}